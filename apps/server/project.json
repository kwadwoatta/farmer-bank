{
  "root": "apps/server",
  "sourceRoot": "apps/server",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go build -o=../../dist/apps/server/ ./server.go",
        "cwd": "apps/server"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "air -c .air.toml",
        "cwd": "apps/server"
      }
    }
  }
}
